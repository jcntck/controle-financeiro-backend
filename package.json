{
  "name": "server",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "dev": "npm run infra:up && nodemon src/main.ts",
    "build": "tsc -p .",
    "start": "env-cmd -f .env npm run build && node ./dist/src/main.js",
    "start:dev": "env-cmd -f .env.development npm run dev",
    "test": "env-cmd -f .env.test jest",
    "test:watch": "env-cmd -f .env.test jest --watch",
    "infra:up": "docker compose -f ./data/compose.yaml up -d",
    "infra:down": "docker compose -f ./data/compose.yaml down",
    "infra:stop": "docker compose -f ./data/compose.yaml stop"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "supertest": "^6.3.3"
  }
}
